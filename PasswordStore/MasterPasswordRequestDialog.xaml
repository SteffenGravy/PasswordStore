<Window x:Class="PasswordStore.MasterPasswordRequestDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Master password request" Height="120" Width="300">
    <StackPanel Margin="10">
        <Label>Master password:</Label>
        <PasswordBox MaxLength="20"/>
        <Button HorizontalAlignment="Center" Command="{Binding MasterPasswordCmd}" CommandParameter="{Binding ElementName=psWord}" Margin="0,5,0,0 ">Ok</Button>
    </StackPanel>
</Window>
